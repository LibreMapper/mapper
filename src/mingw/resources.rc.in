 // Do not edit resources.rc! Edit resources.rc.in instead.

 // Cf. http://msdn.microsoft.com/en-us/library/aa381058%28v=vs.85%29

IDI_ICON1 ICON "Mapper.ico"

1 VERSIONINFO
FILEVERSION    @Mapper_VERSION_MAJOR@,@Mapper_VERSION_MINOR@,@Mapper_VERSION_PATCH@,0
PRODUCTVERSION @Mapper_VERSION_MAJOR@,@Mapper_VERSION_MINOR@,@Mapper_VERSION_PATCH@,0
BEGIN
	BLOCK "StringFileInfo"
	BEGIN
		BLOCK "080904E4"
		BEGIN
			VALUE "CompanyName", "OpenOrienteering Developers"
			VALUE "FileDescription", "OpenOrienteering @CMAKE_PROJECT_NAME@ @Mapper_VERSION_MAJOR@.@Mapper_VERSION_MINOR@.@Mapper_VERSION_PATCH@"
			VALUE "FileVersion", "@Mapper_VERSION_MAJOR@.@Mapper_VERSION_MINOR@.@Mapper_VERSION_PATCH@ @CMAKE_BUILD_TYPE@"
			VALUE "InternalName", "@CMAKE_PROJECT_NAME@"
			VALUE "LegalCopyright", "@Mapper_COPYRIGHT@"
			VALUE "OriginalFilename", "Mapper.exe"
			VALUE "ProductName", "OpenOrienteering @CMAKE_PROJECT_NAME@"
			VALUE "ProductVersion", "@Mapper_VERSION_MAJOR@.@Mapper_VERSION_MINOR@.@Mapper_VERSION_PATCH@ @CMAKE_BUILD_TYPE@"
		END
	END

	BLOCK "VarFileInfo"
	BEGIN
    	VALUE "Translation", 0x809, 1252
	END
END
